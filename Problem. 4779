# Baekjoon 4779번: 칸토어 집합

import sys
input = sys.stdin.readline

T = int(input())
P = [1, 1, 1, 2, 2, 3, 4]

def solution():
    white = [1, 2, 4]
    blue = [1, 2, 3]
    
    for i in range(7, 101):
        if i % 2 == 0:
            P.append(blue[-1] + blue[-3])
            white.append(blue[-1] + blue[-3])
        else:
            P.append(white[-1] + white[-3])
            blue.append(white[-1] + white[-3])

solution()
for i in range(T):
    print(P[int(input())-1])
